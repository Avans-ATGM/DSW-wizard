            "icon": "far fa-file-code",
            "color": "#f15722",
            "steps": [
                {
                    "name": "jinja",
                    "options": {
                        "template": "src/index.html.j2",
                        "content-type": "text/html",
                        "extension": "html"
                    }
                }
            ]
        },
        {
