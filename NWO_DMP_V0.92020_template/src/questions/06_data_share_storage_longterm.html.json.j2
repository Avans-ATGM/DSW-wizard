{%- set ADatamadeAvailableReUseOTHER = repliesMap[QAamDatamadeAvailableReUseOTHERpath]|reply_str_value -%}

{%- set WhenDataAvailablePath = [UUID.chapter6UUID,UUID.WhenDataAvailableUUID]|reply_path -%}
{%- set mWhenDataAvailable = repliesMap[WhenDataAvailablePath]|reply_str_value -%}
{%- set AmWhenDataAvailable = km.entities.answers[mWhenDataAvailable] -%}

{%- set RepositoryDatapath = [UUID.chapter6UUID,UUID.RepositoryDataUUID]|reply_path -%}
{%- set ARepositoryData = repliesMap[RepositoryDatapath]|reply_str_value -%}

{%- set StratagyPublishingpath = [UUID.chapter6UUID,UUID.StratagyPublishingUUID]|reply_path -%}
{%- set AStratagyPublishing = repliesMap[StratagyPublishingpath]|reply_str_value -%}
		


		{% if amDataSelectionLongtermPresevation %}
                	<p> How will data be selected for long-term preservation: {{ AamDataSelectionLongtermPresevation.label }} <p>
