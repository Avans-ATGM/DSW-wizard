{%- set legalRestrictionsAccessDatapath = [UUID.chapter6UUID,UUID.legalRestrictionsAccessDataUUID]|reply_path -%}
{%- set amlegalRestrictionsAccessDatapath = repliesMap[legalRestrictionsAccessDatapath]|reply_str_value -%}
{%- set AamlegalRestrictionsAccessData = km.entities.answers[amlegalRestrictionsAccessDatapath] -%}

{%- set AamlegalRestrictionsAccessDataYESpath= [legalRestrictionsAccessDatapath,UUID.AamlegalRestrictionsAccessDataYESUUID,UUID.QlegalRestrictionsAccessDataYESUUID]|reply_path -%}
